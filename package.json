{
  "name": "changegittogoogledrive-extension",
  "displayName": "ChangeGitToGoogleDrive Extension",
  "description": "An extension to archive and upload workspace files to Google Drive.",
  "version": "0.0.6",
  "publisher": "user",
  "repository": {
    "type": "git",
    "url": "https://github.com/adva-cg/changegittogoogledrive-extension.git"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:changegittogoogledrive-extension.setupGoogleCredentials",
    "onCommand:changegittogoogledrive-extension.authenticateWithGoogle",
    "onCommand:changegittogoogledrive-extension.initialUpload",
    "onCommand:changegittogoogledrive-extension.incrementalUpload",
    "onCommand:changegittogoogledrive-extension.downloadChanges"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "changegittogoogledrive-extension.initialUpload",
        "title": "Начальная выгрузка в Google Drive",
        "category": "Git-To-Google-Drive"
      },
      {
        "command": "changegittogoogledrive-extension.incrementalUpload",
        "title": "Инкрементальная выгрузка в Google Drive",
        "category": "Git-To-Google-Drive"
      },
      {
        "command": "changegittogoogledrive-extension.downloadChanges",
        "title": "Загрузить изменения из Google Drive",
        "category": "Git-To-Google-Drive"
      },
      {
        "command": "changegittogoogledrive-extension.setupGoogleCredentials",
        "title": "Установить учетные данные Google",
        "category": "Git-To-Google-Drive"
      },
      {
        "command": "changegittogoogledrive-extension.authenticateWithGoogle",
        "title": "Аутентификация с Google",
        "category": "Git-To-Google-Drive"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@vscode/test-electron": "^2.3.4",
    "glob": "^8.1.0",
    "mocha": "^10.2.0"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "googleapis": "^153.0.0",
    "moment": "^2.29.4",
    "unzipper": "^0.10.11"
  }
}